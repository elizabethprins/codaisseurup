

<!-- Main jumbotron for a primary marketing message or call to action -->
<div class="jumbotron">
  <div class="container">
    <h1>Welcome to Codaisseurup!</h1>
    <p>Create and share events.</p>
    <p><a class="btn btn-primary btn-lg" href="#" role="button">Learn more &raquo;</a></p>
  </div>
</div>


<div class="container">
  <!-- Example row of columns -->
  <% @events.each do |event| %>
  <div class="row">
    <div class="col-md-4">
      <img src="data:image/gif;base64,R0lGODlhAQABAIAAAHd3dwAAACH5BAAAAAAALAAAAAABAAEAAAICRAEAOw==" class="img-rounded" alt="Generic placeholder image" width="300" height="200">
      <h2><%= link_to event.name, event_path(event) %></h2>
      <p id="dash"><%= event.description %></p>
      <p><a class="btn btn-default" href="#" role="button">View details &raquo;</a></p>
    </div>
  <% end %>
  </div>

  <hr>

  <footer>
    <p>&copy; 2016 Company, Inc.</p>
  </footer>
</div> <!-- /container -->
